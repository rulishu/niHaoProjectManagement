[
  {
    "path": "/login",
    "component": "@/layouts/UserLayout"
  },
  {
    "path": "/",
    "component": "@/layouts/BasicLayout",
    "routes": [
      {
        "index": true,
        "redirect": "/milestone"
      },
      {
        "path": "/tableList/:id",
        "name": "查询表格2",
        "component": "@/pages/TableList",
        "icon": "search",
        "navigate": "(navigate) => {navigate('/tableList',{ replace: true })}"
      },
      {
        "path": "/issues-center/:id",
        "name": "任务中心",
        "component": "@/pages/IssuesCenter",
        "icon": "wifi",
        "navigate": "(navigate) => {navigate('/issues-center/:id',{ replace: true })}"
      },
      {
        "path": "/home",
        "name": "工作台",
        "component": "@/pages/WorkBench"
      },
      {
        "path": "/TodoList",
        "name": "待办事项列表",
        "component": "@/pages/TodoList",
        "icon": "save"
      },
      {
        "path": "/projectOverview",
        "name": "项目概览",
        "component": "@/pages/ProjectOverview",
        "icon": "home",
        "isAuth": true
      },
      {
        "path": "/milestone",
        "name": "里程碑",
        "component": "@/pages/Milestone",
        "isAuth": true
      },
      {
        "path": "/milestone",
        "name": "里程碑",
        "isAuth": true,
        "hideInMenu": true,
        "navigate": "(navigate) => {navigate(`/milestone/${sessionStorage.getItem('id')}`)}",
        "routes": [
          {
            "path": "/milestone/newMilestone",
            "name": "新增里程碑",
            "component": "@/pages/Milestone/NewMilestone",
            "isAuth": true,
            "navigate": "(navigate) => {navigate(`/milestone/newMilestone/${sessionStorage.getItem('id')}`)}"
          },
          {
            "path": "/milestone/editMilestone",
            "name": "编辑里程碑",
            "component": "@/pages/Milestone/NewMilestone",
            "isAuth": true,
            "navigate": "(navigate) => {navigate(`/milestone/newMilestone/${sessionStorage.getItem('id')}`)}"
          },
          {
            "path": "/milestone/milestoneInfo",
            "name": "里程碑详情",
            "component": "@/pages/Milestone/MilestoneInfo",
            "isAuth": true,
            "navigate": "(navigate) => {navigate(`/milestone/milestoneInfo/${sessionStorage.getItem('id')}`)}"
          }
        ]
      },
      {
        "path": "/task",
        "name": "项目任务",
        "component": "@/pages/Task",
        "isAuth": true
      },
      {
        "path": "/task",
        "name": "项目任务",
        "isAuth": true,
        "hideInMenu": true,
        "navigate": "(navigate) => {navigate(`/milestone/${sessionStorage.getItem('id')}`)}",

        "routes": [
          {
            "path": "/task/newIssue",
            "name": "任务需求",
            "component": "@/pages/NewIssue",
            "hideInMenu": true,
            "isAuth": true
          },
          {
            "path": "/task/taskInfo",
            "name": "任务详情",
            "component": "@/pages/TaskInfo",
            "isAuth": true,
            "navigate": "(navigate) => {navigate(`/task/${sessionStorage.getItem('id')}`)}"
          }
        ]
      },
      {
        "path": "/users",
        "name": "成员管理",
        "component": "@/pages/AllUsers",
        "isAuth": true
      },
      {
        "path": "/team",
        "name": "团队管理",
        "component": "@/pages/TeamManagement",
        "isAuth": true
      },
      {
        "path": "/team",
        "name": "团队管理",
        "hideInMenu": true,
        "isAuth": true,
        "routes": [
          {
            "path": "/team/teamItem/:id",
            "name": "团队详情",
            "component": "@/pages/TeamManagement/TeamItem",
            "hideInMenu": true,
            "isAuth": true
          }
        ]
      },
      {
        "path": "/dictionary",
        "name": "数据字典",
        "component": "@/pages/Dictionary",
        "isAuth": true
      },
      {
        "path": "/Authority",
        "name": "权限管理",
        "isAuth": true,
        "routes": [
          {
            "path": "/Authority/RoleManagement",
            "name": "角色管理",
            "component": "@/pages/Authority/RoleManagement",
            "isAuth": true
          },
          {
            "path": "/Authority/routeManagement",
            "name": "路由管理",
            "component": "@/pages/Authority/RouteManagement",
            "isAuth": true
          }
        ]
      },
      {
        "path": "/403",
        "name": "403",
        "hideInMenu": true,
        "component": "403"
      },
      {
        "path": "/500",
        "name": "500",
        "hideInMenu": true,
        "component": "500"
      },
      {
        "path": "/404",
        "name": "404",
        "hideInMenu": true,
        "component": "404"
      },
      {
        "path": "*",
        "name": "404",
        "component": "404"
      }
    ]
  }
]
